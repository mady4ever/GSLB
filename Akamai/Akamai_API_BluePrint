                    "datacenterId": 5400,
                    "nickname": "All Other CIDR Blocks"
                }
                "name": "The North"
            }

    + Schema

            {
                "$schema" :  "http://json-schema.org/draft-03/schema#",
                "type" : "object",
                "additionalProperties": false,
                "properties" :     {
                    "name" : {
                        "type" : "string",
                        "minLength": 1,
                        "maxLength": 255
