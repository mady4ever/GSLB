                                "datacenterId" : {
                                    "type" : "integer",
                                    "required": true
                                },
                                "nickname" : {
                                    "type" : "string",
                                    "required" : true,
                                    "minLength": 1,
                                    "maxLength": 256
                                },
                                "blocks" : {
                                    "type" : "array",
                                    "items" : {
                                        "type" : "string",
                                        "minLength": 1,
                                        "maxLength": 255
                                    }
                                }
                            }
                        }
                    },
                    "defaultDatacenter" : {
                        "type" : "object",
                        "additionalProperties": false,
                        "required": true,
                        "properties" : {
                            "datacenterId" : {
                                "type" : "integer",
                                "required": true
                            },
                            "nickname" : {
                                "type" : "string",
                                "minLength": 1,
                                "maxLength": 128
            
                            }
                        }
                    },
                    "links" : {
                        "type" : "array",
                        "items" : {
                            "type" : "object",
                            "properties" : {
                                "rel" : {
