            }

### Create or update a Resource [PUT]
+ Request (application/json)

    + Body

            {
                "aggregationType": "latest",
                "constrainedProperty": "mail",
                "decayRate": null,
                "description": "Disk utilization",
                "hostHeader": null,
                "leaderString": null,
                "leastSquaresDecay": null,
                "loadImbalancePercentage": null,
                "maxUMultiplicativeIncrement": null,
                "name": "disk",
                "resourceInstances": [
                    {
                        "datacenterId": 3134,
                        "loadObject": "/disk",
                        "loadObjectPort": 80,
                        "loadServers": [
                            "1.2.3.9"
                        ],
                        "useDefaultLoadObject": false
                    },
                    {
                        "datacenterId": 3133,
                        "loadObject": "/disk",
                        "loadObjectPort": 80,
                        "loadServers": [
                            "1.2.3.10"
                        ],
                        "useDefaultLoadObject": false
                    }
                ],
                "type": "XML load object via HTTP",
                "upperBound": 0
            }


    + Schema

            {
                "$schema" :  "http://json-schema.org/draft-03/schema#",
                "type" : "object",
                "additionalProperties": false,
                "properties" : {
                    "leastSquaresDecay" : {
                        "type" : "number",
                        "minimum": 0,
                        "maximum": 1
                    },
                    "hostHeader" : {
                        "type" : "string",
                        "maxLength" : 255
                    },
                    "description" : {
                        "type" : "string",
                        "maxLength" : 256
                    },
                    "leaderString" : {
                        "type" : "string",
                        "maxLength" : 256
                    },
                    "constrainedProperty" : {
                        "type" : "string",
                        "maxLength": 256
                    },
                    "resourceInstances" : {
                        "type" : "array",
                        "items" : {
                            "type" : "object",
                            "properties" : {
                                "datacenterId" : {
                                    "type" : "integer",
                                    "required": true
                                },
                                "useDefaultLoadObject" : {
                                    "type" : "boolean"
                                },
                                "loadServers" : {
                                    "type" : "array",
                                    "items" : {
                                        "type" : "string"
                                    }
                                },
                                "loadObject" : {
                                    "type" : "string",
                                    "maxLength" : 256,
                                    "pattern" : "^(?!:).*$"
                                },
                                "loadObjectPort" : {
                                    "type" : "integer",
                                    "minimum": 0,
                                    "maximum": 65535
                                }
                            }
                        }
                    },
                    "aggregationType" : {
                        "type" : "string",
                        "maxLength": 15,
                        "required" : true,
                        "enum" :  ["sum", "median", "latest"]
                    },
                    "links" : {
                        "type" : "array",
                        "items" : {
                            "type" : "object",
                            "properties" : {
                                "rel" : {
                                    "type" : "string"
                                },
                                "href" : {
                                    "type" : "string"
                                }
                            }
                        }
                    },
                    "loadImbalancePercentage" : {
                        "type" : "number",
                        "minimum": 0,
                        "maximum": 1000000
                    },
                    "type" : {
                        "type" : "string",
                        "required" : true,
                        "enum" :  ["XML load object via HTTP", "XML load object via HTTPS", "Non-XML load object via HTTP", "Non-XML load object via HTTPS", "Download score"]
                    },
                    "upperBound" : {
                        "type" : "integer"
                    },
                    "name" : {
                        "type" : "string",
                        "required": true,
                        "maxLength": 150,
                        "pattern" : "^((?!\\s).)*$"
                    },
                    "maxUMultiplicativeIncrement" : {
                        "type" : "number",
                        "minimum" : 1.0
                    },
                    "decayRate" : {
                        "type" : "number",
                        "minimum": 0,
                        "maximum": 1
                    }
                }
            }

+ Response 200 (application/json)

            {
                "resource": {
                    "aggregationType": "latest",
                    "constrainedProperty": "mail",
