### Create a Datacenter [POST]
+ Request (application/json)
    + Body

             {
                "city": "Doune",
                "stateOrProvince": "Perthshire",
                "cloneOf": 0,
                "continent": "EU",
                "country": "GB",
                "defaultLoadObject": {
                    "loadObject": null,
                    "loadObjectPort": 0,
                    "loadServers": null
                },
                "latitude": 56.185096,
                "longitude": -4.050264,
                "nickname": "Winterfell"
            }

    + Schema

            {
                "$schema" :  "http://json-schema.org/draft-03/schema#",
                "type": "object",
                "properties": {
                    "city": {
                        "type": "string"
                    },
                    "cloneOf": {
                        "type": "integer"
                    },
                    "continent": {
                        "type": "string",
                        "maxLength": 2,
                        "enum": ["AF", "AS", "EU", "NA", "OC", "OT", "SA"]
                    },
                    "country": {
                        "type": "string",
                        "maxLength": 2
                    },
                    "defaultLoadObject": {
                        "type": "object",
                        "properties": {
                            "loadObject": {
                                "type": "string",
                                "maxLength" : 256,
                                "pattern" : "^(?!:).*$"
                            },
                            "loadObjectPort": {
                                "type": "integer",
                                "minimum": 0,
                                "maximum": 65535
                            },
                            "loadServers": {
                                "items": {
                                    "type": "string",
                                    "maxLength": 256
                                },
                                "type": "array"
                            }
                        }
                    },
                    "latitude": {
                        "type": "number"
                    },
                    "links": {
                        "items": {
                            "properties": {
                                "href": {
                                    "type": "string"
                                },
                                "rel": {
                                    "type": "string"
                                }
                            },
                            "type": "object"
                        },
                        "type": "array"
                    },
                    "longitude": {
                        "type": "number"
                    },
                    "nickname": {
                        "type": "string"
                    },
                    "datacenterId": {
                        "required": true,
                        "type": "integer"
                    },
                    "stateOrProvince": {
                        "type": "string"
                    },
                    "virtual": {
                        "type": "boolean"
                    }
                },
                "additionalProperties": false
            }

+ Response 201 (application/json)

    + Headers

            Location: /config-gtm/v1/domains/example.akadns.net/datacenters/3133

    + Body

            {
                "resource": {
                    "city": "Doune",
                    "cloneOf": 0,
                    "continent": "EU",
                    "country": "GB",
                    "datacenterId": 3133,
                    "defaultLoadObject": {
                        "loadObject": null,
                        "loadObjectPort": 0,
                        "loadServers": null
                    },
                    "latitude": 56.185097,
                    "links": [
                        {
                            "href": "/config-gtm/v1/domains/example.akadns.net/datacenters/3133",
                            "rel": "self"
                        }
                    ],
                    "longitude": -4.050264,
