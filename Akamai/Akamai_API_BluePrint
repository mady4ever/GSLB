                            }
                        }
                    },
                    "links" : {
                        "type" : "array",
                        "items" : {
                            "type" : "object",
                            "properties" : {
                                "rel" : {
                                    "type" : "string"
                                },
                                "href" : {
                                    "type" : "string"
                                }
                            }
                        }
                    }
                }
            }

